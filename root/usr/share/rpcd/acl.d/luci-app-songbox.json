{
    "luci-app-songbox": {
        "description": "Allow access to the SongBox application",
        "read": {
            "uci": [ "songbox" ],
            "file": {
                "/var/run/songbox/run.log": [ "read" ],
                "/var/run/songbox/singbox.log": [ "read" ],
                "/usr/libexec/rpcd/songbox/*": [ "exec"]
            }
        },
        "write": {
            "uci": [ "songbox" ],
            "file": {
                "/var/run/songbox/run.log": [ "write" ],
                "/var/run/songbox/singbox.log": [ "write" ]
            }
        }
    }
}