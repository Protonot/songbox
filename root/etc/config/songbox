config client 'client'
	option enabled '0'
	option singbox_mode '0'

config log 'log'
	option disabled 'false'
	option level 'info'
	option output '/var/run/songbox/singbox.log'
	option timestamp 'true'

config ntp 'ntp'
	option enabled 'false'
	option server 'time.apple.com'
	option server_port '123'
	option interval '30m'

config experimental 'experimental'
	option enabled 'true'
	option store_fakeip 'true'
	option store_rdrc 'false'
	option external_controller ''
	option secret ''
	option external_ui '/var/etc/singbox/ui'
	option external_ui_download_url 'https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip'

config dns_fakeip 'dns_fakeip'
	option enabled 'true'
	option inet4_range '198.18.0.0/15'

config dns_advanced 'dns_advanced'
	option final 'dns_direct'
	option independent_cache 'true'

config route_advanced 'route_advanced'
	option auto_detect_interface 'true'

config subscription 'subscription'

config node 'node'